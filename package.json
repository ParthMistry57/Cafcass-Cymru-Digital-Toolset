{
  "name": "cafcass",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "icon": "icon",
  "homepage": "./",
  "dependencies": {
    "@react-spring/web": "^9.7.4",
    "@testing-library/user-event": "^13.5.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.12.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "jspdf": "^2.5.1",
    "merge-images": "^2.0.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-sketch-canvas": "^6.2.0",
    "react-slider": "^2.0.6",
    "uuid": "^10.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "package-mac": "electron-packager . Cafcass --platform=darwin --arch=arm64,x64 --overwrite --out=release-builds --icon=icon.icns",
    "package-windows": "electron-packager . Cafcass --platform=win32 --arch=ia32,x64 --overwrite --out=release-builds --icon=icon.ico",
    "package-auto": "node build.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.yourapp.id",
    "productName": "Cafcass",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "icon.icns"
    },
    "win": {
      "icon": "icon.ico"
    },
    "linux": {
      "icon": "icon.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "electron": "^32.0.1",
    "electron-packager": "^17.1.2"
  }
}